        "arr = list(map(int, input().split()))\n",
        "def Fibonacci(n):\n",
        "   if n <= 1:\n",
        "       return n\n",
        "   else:\n",
        "       return(Fibonacci(n-1) + Fibonacci(n-2))\n",
        "\n",
        "k = 2*(arr[1]) + 1\n",
        "y = Fibonacci(k)*arr[0]\n",
        "print(y)"
