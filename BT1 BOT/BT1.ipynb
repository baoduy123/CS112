   "source": [
    "x = int(input())\n",
    "arr = list(map(int, input().split()))\n",
    "ans = 0\n",
    "sum = 0\n",
    "p = 0\n",
    "q = 0\n",
    "s = 0\n",
    "for i in range(x):\n",
    "    if sum < 0:\n",
    "        s = i\n",
    "        sum = arr[i]\n",
    "    else:\n",
    "        sum += arr[i]\n",
    "    if ans < sum:\n",
    "        ans = sum\n",
    "        q = i\n",
    "        p = s\n",
    "print(p + 1, q + 1, ans)"
   ]
