        "x = int(input())\n",
        "i = 0\n",
        "sum = 0\n",
        "arr = []\n",
        "ans = 0\n",
        "while i < x:\n",
        "    arr.append(int(input()))\n",
        "    sum = sum + arr[i]\n",
        "    if ans < sum:\n",
        "        ans = sum\n",
        "        q = i\n",
        "    if arr[i] > sum:\n",
        "        sum = arr[i]\n",
        "        p = i\n",
        "    i = i + 1\n",
        "\n",
        "print(\"\\n\")\n",
        "print(p + 1)\n",
        "print(q + 1)\n",
        "print(ans)"
      ],
